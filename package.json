  },
  "homepage": "https://github.com/doing123/imooc-react-webpack#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "react": "^16.5.0",
    "react-dom": "^16.5.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
